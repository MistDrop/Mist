mist.dev {
  root /var/www/mist.dev/static

  log /var/log/casket/mist.dev.access.log
  errors /var/log/casket/mist.dev.error.log {
    502 down.html
  }

  proxy / 127.0.0.1:8080 {
    transparent
    except /style.css
  }

  cors
  gzip
}

ws.mist.dev {
  root /var/www/mist.dev/static
  
  log /var/log/casket/ws.mist.dev.access.log
  errors /var/log/casket/ws.mist.dev.error.log {
    502 down.html
  }

  proxy /ws/gateway 127.0.0.1:8080 {
    transparent
    websocket
  }

  cors
  gzip
}
